{
  "name": "@redwoodjs/starter",
  "version": "1.0.0",
  "description": "A bare-bones RedwoodSDK starter",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "migrate": "tsx scripts/run-migrate-d1.ts",
    "migrate:generate": "drizzle-kit generate",
    "migrate:apply:d1": "tsx scripts/run-migrate-d1.ts",
    "seed": "tsx scripts/seed.ts",
    "seed:demo": "tsx scripts/seed-demo.ts",
    "shadcn:init": "npx shadcn@latest init",
    "shadcn:add": "npx shadcn@latest add",
    "check": "npm run generate && npm run types",
    "types": "tsc"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.548.0",
    "nanoid": "^5.1.6",
    "ofetch": "^1.5.0",
    "ohash": "^2.0.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-server-dom-webpack": "19.2.0",
    "rwsdk": "1.0.0-beta.22",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "ufo": "^1.6.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.13.16",
    "@cloudflare/workers-types": "^4.20251014.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.22",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.6",
    "postcss": "^8.5.6",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "wrangler": "4.45.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}